    <logger id="tracelog" fileName="WN.log" logEvery="10000" model="@posterior" sanitiseHeaders="true" sort="smart">
      <log idref="posterior"/>
      <log idref="likelihood"/>
      <log idref="prior"/>
      <log idref="WNVars"/>
      <log idref="WNMeans"/>
	  <log idref="WNRegimes"/>
    </logger>

    <logger id="screenlog" logEvery="10000">
      <log idref="posterior"/>
      <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
      <log idref="likelihood"/>
      <log idref="prior"/>
